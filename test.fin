struct Vec
    x Int
    y Int
.

def vec x Int y Int -> Vec as
    'Vec x y
.

def fib n Int -> Int as
    if n = 0 then
        0
    elif n = 1 then
        1
    else
        'fib (n - 1) + 'fib (n - 2)
    .
.

def main -> Int as
    'vec 3 4 -> let v,
    if 20 -> let i, i > 0 then
        'fib i
    else
        0
    .
.

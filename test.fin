extern getchar -> Int

extern putchar ch Int -> Int

struct Vec as
    x Int
    y Int
.

def len v Vec -> Int as
    (v 'x * v 'x) + (v 'y * v 'y)
.

def fib n Int -> Int as
    if n == 0 then
        0
    elif n == 1 then
        1
    else
        'fib (n - 1) + 'fib (n - 2)
    .
.

def encrypt shift Int as
    let Int -> ch,
    'getchar = ch,
    if *ch != -1 then
        *ch + shift = ch,
        'putchar *ch,
        'encrypt shift
    .
.

def main -> Int as
    'encrypt 1,
    'Vec 3 4 -> v,
    if 'len v -> i, !(i < 0) then
        'fib i
    else
        0
    .
.

struct Vec
    x Int
    y Int
.

def vec x Int y Int -> Vec as
    'Vec x y
.

def len v Vec -> Int as
    (v 'x * v 'x) + (v 'y * v 'y)
.

def fib n Int -> Int as
    if n = 0 then
        0
    elif n = 1 then
        1
    else
        'fib (n - 1) + 'fib (n - 2)
    .
.

